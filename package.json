{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 3000 --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Vincent Fillon",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "@types/jquery": "^3.3.29",
    "@types/node": "^11.11.1",
    "autoprefixer": "^9.4.10",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "font-awesome-loader": "^1.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.5",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.13.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "dexie": "^2.0.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "rxjs": "^6.4.0"
  }
}
